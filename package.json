{
  "name": "ExpressionEngine",
  "repository": "https://github.com/ExpressionEngine/ExpressionEngine",
  "private": true,
  "scripts": {
    "build:js": "gulp buildJs",
    "watch:js": "gulp watchJs",
    "build:old-js": "babel themes/ee/asset/javascript/src/ -x \".es6\" --out-dir themes/ee/asset/javascript/src/",
    "watch:old-js": "npm run build:old-js -- --watch",
    "build:css": "brunch build && cd \"cp-styles\" && (cp build/common.min.css ../themes/ee/cp/css/common.min.css || copy build\\common.min.css ..\\themes\\ee\\cp\\css\\common.min.css) && (cp build/eecms-debug.min.css ../themes/ee/debug/css/eecms-debug.min.css || copy build\\eecms-debug.min.css ..\\themes\\ee\\debug\\css\\eecms-debug.min.css) && (cp -a build/webfonts/. ../themes/ee/webfonts/ || copy build\\webfonts ..\\themes\\ee\\webfonts)",
    "watch:css": "brunch watch --server",
    "build:theme": "brunch build --production",
    "watch:theme": "brunch watch --server"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@types/jquery": "^3.3.31",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "auto-reload-brunch": "^2",
    "autoprefixer": "^10.4.4",
    "babel-brunch": "^7",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.27.9",
    "brunch": "^2.10.17",
    "clean-css-brunch": "^2.10.0",
    "cssnano": "^5.1.7",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-purgecss": "^4.1.3",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^5.0.1",
    "gulp-uglify": "^1.4.0",
    "less-brunch": "^2.10.0",
    "postcss": "^8.4.12",
    "postcss-brunch": "^3.0.0",
    "postcss-import": "^14.1.0",
    "sass": "^1.49.11",
    "sass-brunch": "^3.0.1",
    "typescript": "^3.7.3",
    "typescript-brunch": "^2.3.0",
    "uglify-js-brunch": "^2"
  },
  "dependencies": {
    "@waaark/luge": "^0.6.7-beta",
    "accoutrement": "^4.0.1",
    "gerillass": "^1.3.0",
    "gsap": "^3.10.2",
    "jquery": "^3.6.0",
    "swiper": "^8.0.7",
    "tailwindcss": "^3.0.23"
  }
}
